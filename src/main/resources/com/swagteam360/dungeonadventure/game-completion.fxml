<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.swagteam360.dungeonadventure.controller.GameCompletionController">
   <children>
      <VBox alignment="CENTER" prefHeight="480.0" prefWidth="640.0">
         <children>
            <Label text="Dungeon Map">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <GridPane fx:id="myDungeonGrid" alignment="CENTER" prefHeight="392.0" prefWidth="640.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="40.0" minWidth="10.0" prefWidth="40.0" />
              </columnConstraints>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <rowConstraints>
                  <RowConstraints maxHeight="40.0" minHeight="10.0" prefHeight="40.0" />
               </rowConstraints>
            </GridPane>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="640.0" spacing="25.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#newGame" prefWidth="100.0" text="New Game" />
                  <Button mnemonicParsing="false" onAction="#quitGame" prefWidth="100.0" text="Quit" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
