<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.swagteam360.dungeonadventure.controller.StartMenuController">
   <children>
      <Label alignment="CENTER" layoutY="95.0" prefHeight="74.0" prefWidth="640.0" text="Dungeon Adventure" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="56.0" />
         </font>
      </Label>
      <Separator layoutX="220.0" layoutY="323.0" prefWidth="200.0" />
      <MenuButton layoutY="1.0" mnemonicParsing="false" text="Help" underline="true">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#aboutUsMenuEvent" text="About Us" />
            <MenuItem fx:id="howToPlayAction" mnemonicParsing="false" onAction="#howToPlayMenuEvent" text="How to Play" />
        </items>
      </MenuButton>
      <HBox layoutX="232.0" layoutY="336.0" prefHeight="49.0" prefWidth="175.0">
         <children>
            <Button mnemonicParsing="false" onAction="#startButtonEvent" prefHeight="40.0" prefWidth="75.0" text="Start">
               <HBox.margin>
                  <Insets right="25.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#quitButtonEvent" prefHeight="40.0" prefWidth="75.0" text="Quit">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
